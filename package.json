{
  "name": "@accs/cms",
  "version": "0.1.0",
  "private": true,
  "description": "A Strapi application",
  "scripts": {
    "build": "strapi build",
    "console": "strapi console",
    "deploy": "strapi deploy",
    "login": "strapi login",
    "dev": "strapi develop",
    "develop": "strapi develop",
    "start": "strapi start",
    "strapi": "strapi",
    "upgrade": "npx @strapi/upgrade latest",
    "upgrade:dry": "npx @strapi/upgrade latest --dry",
    "postinstall": "npm run build:plugins",
    "build:plugins": "npm run build:plugin-import && npm run build:plugin-vision",
    "build:plugin-import": "cd src/plugins/import-puchased-accounts && npm install && npm run build",
    "build:plugin-vision": "cd src/plugins/create-vision-profile && npm install && npm run build"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@strapi/plugin-cloud": "5.13.0",
    "@strapi/plugin-users-permissions": "5.13.0",
    "@strapi/strapi": "5.13.0",
    "axios": "^1.6.0",
    "inversify": "^7.7.0",
    "otplib": "^12.0.1",
    "pg": "8.8.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "reflect-metadata": "^0.2.2",
    "styled-components": "^6.0.0",
    "totp-generator": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0 <=22.x.x",
    "npm": ">=6.0.0"
  },
  "strapi": {
    "uuid": "41b67835-a9ac-4d79-9472-1124df0dd182",
    "installId": "1abeac338b4f1671e39c6d6f8c2ab481e3be0b2f560e5d6720c8df11b0f96d77"
  },
  "nx": {
    "name": "cms",
    "targets": {
      "build": {
        "executor": "@nx/esbuild:esbuild",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "platform": "node",
          "outputPath": "apps/cms/dist",
          "format": [
            "cjs"
          ],
          "bundle": false,
          "main": "apps/cms/src/main.ts",
          "tsConfig": "apps/cms/tsconfig.app.json",
          "assets": [
            "apps/cms/src/assets"
          ],
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".js"
            }
          }
        },
        "configurations": {
          "development": {},
          "production": {
            "esbuildOptions": {
              "sourcemap": false,
              "outExtension": {
                ".js": ".js"
              }
            }
          }
        }
      },
      "serve": {
        "continuous": true,
        "executor": "@nx/js:node",
        "defaultConfiguration": "development",
        "dependsOn": [
          "build"
        ],
        "options": {
          "buildTarget": "cms:build",
          "runBuildTargetDependencies": false
        },
        "configurations": {
          "development": {
            "buildTarget": "cms:build:development"
          },
          "production": {
            "buildTarget": "cms:build:production"
          }
        }
      }
    }
  }
}
